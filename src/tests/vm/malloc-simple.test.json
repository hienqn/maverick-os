    {"type": "regex", "pattern": "\\(malloc-simple\\) freed all 20 blocks", "message": "Missing freed blocks message"},
    {"type": "regex", "pattern": "\\(malloc-simple\\) malloc\\(0\\) handled correctly", "message": "Missing malloc(0) message"},
